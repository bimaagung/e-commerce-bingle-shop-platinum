{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-await-in-loop": "off",
        "no-continue":"off",
        "prefer-const":"off",
        "consistent-return":"off",
        "linebreak-style":"off"
    }
}
